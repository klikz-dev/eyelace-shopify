    "text_columns_UgCPm9": {
      "type": "text-columns",
      "blocks": {
        "image_QU8yP7": {
          "type": "image",
          "settings": {
            "show-image": true,
            "image": "shopify:\/\/shop_images\/Step_1_d846c517-6e1b-45c0-887f-8550cca955fd.gif",
            "title": "",
            "description": "<p>To remove your Eylace, gently pull it until the glue slowly separates from your natural lash. <\/p>",
            "link-url": "",
            "link-text": ""
          }
        },
        "image_wYQEDx": {
          "type": "image",
          "settings": {
            "show-image": true,
            "image": "shopify:\/\/shop_images\/Step_2_28c9b56f-a1be-4b7a-b6fd-be1812ee501f.gif",
            "title": "",
            "description": "<p>If you feel any tension use a warm compress, or a shower to melt the glue before you pull.<\/p>",
            "link-url": "",
            "link-text": ""
          }
        },
        "image_BVth8f": {
          "type": "image",
          "settings": {
            "show-image": true,
            "image": "shopify:\/\/shop_images\/Step_3_1b2c84b2-e8a4-40a0-8f3a-bc88a1d20d2f.gif",
            "title": "",
            "description": "<p>Once removed store your lashes on the plastic tray to reuse them.<\/p>",
            "link-url": "",
            "link-text": ""
          }
        }
      },
